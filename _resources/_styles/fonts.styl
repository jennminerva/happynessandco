@font-face {
	font-family: 'Canela Web';
	src: url('CanelaWeb-Light.eot');
	src: url('CanelaWeb-Light.eot?#iefix') format('embedded-opentype'),
		url('CanelaWeb-Light.woff') format('woff'),
		url('CanelaWeb-Light.ttf') format('truetype');
	font-weight: 300;
	font-style: normal;
}

@font-face {
  // was 'Canela Web'
	font-family: 'Canela Web Italic';
	src: url('CanelaWeb-LightItalic.eot');
	src: url('CanelaWeb-LightItalic.eot?#iefix') format('embedded-opentype'),
		url('CanelaWeb-LightItalic.woff') format('woff'),
		url('CanelaWeb-LightItalic.ttf') format('truetype');
	font-weight: 300;
	font-style: italic;
}

