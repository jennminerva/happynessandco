#FoodMenu {
}
.FoodMenuContainer p {
	margin: 2rem 0 0 0;
}
.FoodMenuPricing {
	display: flex;
	flex-wrap: wrap;
	justify-content: space-between;
	list-style-type: none;
	font-size: 1.5rem;
	margin: 4rem 0;
	padding: 0;
	line-height: 175%;
}
@media tablet {
	.FoodMenuPricing {
		margin: 4rem 15rem;
	}
}
@media desktop {
	.FoodMenuPricing {
		margin: 4rem 0;
	}
}
.FoodMenuPricing li{
	width: 50%;
}
.FoodMenuList {
	margin: 9rem 5rem;
	position: relative;
	top: 4rem;
	clear: both;
}
@media tablet {
	.FoodMenuList {
		margin: 9rem 11rem;
		}
}
.FoodMenuList dt {
	font-size: 1.5rem;
	margin: 1rem;
	color: #ff3431;
}
.FoodMenuList dd {
	margin: 0 0 4rem 3rem;
	line-height: 150%;
}
