html {
	font-size: 10px;
	font-family: typeface-primary, serif;
}
@media laptop {
	body {
		font-size: 10px;
	}
}
.unselectable {
	-moz-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
}
p {
	line-height: 150%;
	margin: -.5em 0 1em 0;
	text-align: left;
	cursor: default;
	font-size: 1.3rem;
}
a, a:focus, a:visited, a:active {
	text-decoration: none;
  color: #FE6D6A;
  border-bottom: 1px solid 
}
strong {
	font-weight: normal;
}
h1,h2,h3,h4,h5,h6 {
	margin: 0 0;
	padding: 0 0;
	font-size: 1rem;
	font-weight: normal;
	line-height: 125%;
	position: relative;
	z-index: 9999;
	cursor: default;
}
.HeadingSmall {
	font-size: 2rem;
	color: #ff3431;
}
.CircularStd {
	font-family: 'CircularStd', sans-serif;
	font-size: 1.3rem;
}
.AlignRight {
	text-align: right;
}
